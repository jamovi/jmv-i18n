"#    cyl:\n"
"#    6 – 4                  0.693        1.22        0.566    0.571\n"
"#    8 – 4                  2.485        1.04        2.387    0.017\n"
"#    gear:cyl:\n"
"#    (4 – 3):(6 – 4)       -1.386        1.37       -1.012    0.311\n"
"#    (5 – 3):(6 – 4)       -1.386        1.73       -0.800    0.423\n"
"#    (4 – 3):(8 – 4)      -26.867    42247.17    -6.36e -4    0.999\n"
"#    (5 – 3):(8 – 4)       -2.485        1.44       -1.722    0.085\n"
"#  ──────────────────────────────────────────────────────────────────\n"
"#\n"
