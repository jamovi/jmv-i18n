#: descriptives/options.description.R.usage
msgid "data('mtcars')\n"
"dat <- mtcars\n"
"\n"
"# frequency tables can be provided for factors\n"
"dat$gear <- as.factor(dat$gear)\n"
"\n"
"descriptives(dat, vars = vars(mpg, cyl, disp, gear), freq = TRUE)\n"
"\n"
"#\n"
"#  DESCRIPTIVES\n"
"#\n"
"#  Descriptives\n"
"#  ───────────────────────────────────────────\n"
"#               mpg     cyl     disp    gear\n"
"#  ───────────────────────────────────────────\n"
"#    N            32      32      32      32\n"
"#    Missing       0       0       0       0\n"
"#    Mean       20.1    6.19     231    3.69\n"
"#    Median     19.2    6.00     196    4.00\n"
"#    Minimum    10.4    4.00    71.1       3\n"
"#    Maximum    33.9    8.00     472       5\n"
"#  ───────────────────────────────────────────\n"
"#\n"
"#\n"
"#  FREQUENCIES\n"
"#\n"
"#  Frequencies of gear\n"
"#  ────────────────────\n"
"#    Levels    Counts\n"
"#  ────────────────────\n"
"#    3             15\n"
"#    4             12\n"
"#    5              5\n"
"#  ────────────────────\n"
"#\n"
"\n"
"# spliting by a variable\n"
"descriptives(formula = disp + mpg ~ cyl, dat,\n"
"    median=F, min=F, max=F, n=F, missing=F)\n"
"\n"
"# providing histograms\n"
"descriptives(formula = mpg ~ cyl, dat, hist=T,\n"
"    median=F, min=F, max=F, n=F, missing=F)\n"
"\n"
"# splitting by multiple variables\n"
"descriptives(formula = mpg ~ cyl:gear, dat,\n"
"    median=F, min=F, max=F, missing=F)"
msgstr ""
"data('mtcars')\n"
"dat <- mtcars\n"
"\n"
"# frequency tables can be provided for factors\n"
"dat$gear <- as.factor(dat$gear)\n"
"\n"
"descriptives(dat, vars = vars(mpg, cyl, disp, gear), freq = TRUE)\n"
"\n"
"#\n"
"#  DESKRIPTIVSTATISTIK\n"
"#\n"
"#  Deskriptivstatistik\n"
"#  ──────────────────────────────────────────────\n"
"#                   mpg    cyl     disp    gear\n"
"#  ──────────────────────────────────────────────\n"
"#    N               32      32      32      32\n"
"#    Fehlend          0       0       0       0\n"
"#    Mittelwert    20.1    6.19     231    3.69\n"
"#    Median        19.2    6.00     196    4.00\n"
"#    Minimum       10.4    4.00    71.1       3\n"
"#    Maximum       33.9    8.00     472       5\n"
"#  ──────────────────────────────────────────────\n"
"#\n"
"#\n"
"#  HÄUFIGKEITEN\n"
"#\n"
"#  Häufigkeit von gear\n"
"#  ───────────────────\n"
"#    Stufe    Anzahl\n"
"#  ───────────────────\n"
"#    3            15\n"
"#    4            12\n"
"#    5             5\n"
"#  ───────────────────\n"
"#\n"
"\n"
"# Gruppering nach einer Variablen\n"
"descriptives(formula = disp + mpg ~ cyl, dat,\n"
"    median=F, min=F, max=F, n=F, missing=F)\n"
"\n"
"# Histogramme anzeigen\n"
"descriptives(formula = mpg ~ cyl, dat, hist=T,\n"
"    median=F, min=F, max=F, n=F, missing=F)\n"
"\n"
"# Gruppierung nach mehreren Variablen\n"
"descriptives(formula = mpg ~ cyl:gear, dat,\n"
"    median=F, min=F, max=F, missing=F)"

