#: contTablesPaired/options.description.R.usage
msgid "dat <- data.frame(\n"
"    `1st survey` = c('Approve', 'Approve', 'Disapprove', 'Disapprove'),\n"
"    `2nd survey` = c('Approve', 'Disapprove', 'Approve', 'Disapprove'),\n"
"    `Counts` = c(794, 150, 86, 570),\n"
"    check.names=FALSE)\n"
"\n"
"contTablesPaired(formula = Counts ~ `1st survey`:`2nd survey`, data = dat)\n"
"\n"
"#\n"
"#  PAIRED SAMPLES CONTINGENCY TABLES\n"
"#\n"
"#  Contingency Tables\n"
"#  ────────────────────────────────────────────────\n"
"#    1st survey    Approve    Disapprove    Total\n"
"#  ────────────────────────────────────────────────\n"
"#    Approve           794           150      944\n"
"#    Disapprove         86           570      656\n"
"#    Total             880           720     1600\n"
"#  ────────────────────────────────────────────────\n"
"#\n"
"#\n"
"#  McNemar Test\n"
"#  ─────────────────────────────────────────────────────\n"
"#                                Value    df    p\n"
"#  ─────────────────────────────────────────────────────\n"
"#    χ²                           17.4     1    < .001\n"
"#    χ² continuity correction     16.8     1    < .001\n"
"#  ─────────────────────────────────────────────────────\n"
"#\n"
"\n"
"\n"
"# Alternatively, omit the left of the formula (`Counts`) from the\n"
"# formula if each row represents a single observation:\n"
"\n"
"contTablesPaired(formula = ~ `1st survey`:`2nd survey`, data = dat)"
msgstr "dat <- data.frame(\n"
"    `1a enquesta` = c('A favor', 'A favor', 'En contra', 'En contra'),\n"
"    `2a enquesta` = c('A favor', 'En contra', 'A favor', 'En contra'),\n"
"    `Counts` = c(794, 150, 86, 570),\n"
"    check.names=FALSE)\n"
"\n"
"contTablesPaired(formula = Counts ~ `1a enquesta`:`2a enquesta`, data = dat)\n"
"\n"
"#\n"
"#  TAULES DE CONTINGÈNCIA PER A MOSTRES EMPARELLADAS\n"
"#\n"
"#  Taules de contingència\n"
"#  ────────────────────────────────────────────────\n"
"#    1a enquesta    A favor    En contra    Total\n"
"#  ────────────────────────────────────────────────\n"
"#    A favor          794         150        944\n"
"#    En contra         86         570        656\n"
"#    Total            880         720       1600\n"
"#  ────────────────────────────────────────────────\n"
"#\n"
"#\n"
"#  Prova de McNemar\n"
"#  ─────────────────────────────────────────────────────\n"
"#                                Valor    g.l.    p\n"
"#  ─────────────────────────────────────────────────────\n"
"#    χ²                           17,4     1    <  ,001\n"
"#    χ² correcció continuïtat     16,8     1    <  ,001\n"
"#  ─────────────────────────────────────────────────────\n"
"#\n"
"\n"
"\n"
"# Alternativament, ometre la part esquerra de la formula (`Counts`)\n"
"# si cada fila representa una observació singular:\n"
"\n"
"contTablesPaired(formula = ~ `1a enquesta`:`2a enquesta`, data = dat)"

