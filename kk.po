#: anovaRM/options.description.R.usage
msgid "data('bugs', package = 'jmv')\n"
"\n"
"anovaRM(\n"
"    data = bugs,\n"
"    rm = list(\n"
"        list(\n"
"            label = 'Frightening',\n"
"            levels = c('Low', 'High'))),\n"
"    rmCells = list(\n"
"        list(\n"
"            measure = 'LDLF',\n"
"            cell = 'Low'),\n"
"        list(\n"
"            measure = 'LDHF',\n"
"            cell = 'High')),\n"
"    rmTerms = list(\n"
"        'Frightening'))\n"
"\n"
"#\n"
"#  REPEATED MEASURES ANOVA\n"
"#\n"
"#  Within Subjects Effects\n"
"#  ───────────────────────────────────────────────────────────────────────\n"
"#                  Sum of Squares    df    Mean Square    F       p\n"
"#  ───────────────────────────────────────────────────────────────────────\n"
"#    Frightening              126     1         126.11    44.2    < .001\n"
"#    Residual                 257    90           2.85\n"
"#  ───────────────────────────────────────────────────────────────────────\n"
"#    Note. Type 3 Sums of Squares\n"
"#\n"
"#\n"
"#\n"
